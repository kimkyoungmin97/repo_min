<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.a5a5lab.module.user.UserDao">
<resultMap id="resultMapObj" type="com.a5a5lab.module.user.UserDto"></resultMap>
<sql id="selectCommon">
	FROM
		user
</sql>
<select id="selectOneCount">
	SELECT 
		count(*)
	<include refid="selectCommon"/>
</select>
<select id="selectList" resultMap="resultMapObj">
	SELECT
		userSeq,
		userName,
		genderCd,
		userBorn,
		telAgencyCd,
		userTel,
		userId,
		userPw,
		userEmail,
		userNickName,
		userAddress
	<include refid="selectCommon"/>
	ORDER BY userSeq DESC
</select>



		


	  
</mapper>